@{
    var trending = ViewBag.Trending;
    var hot = ViewBag.Hot;
    var utilization = ViewBag.Utilization;
}
<h1>Analytics Results</h1>

<h2>Trending</h2>
@foreach (var item in trending)
{
    <p>Trending MediaID: @item</p>
}

@*<h2>Hot</h2>
@foreach (var item in hot)
{
    <p>Hot MediaID: @item</p>
}*@
<h2>Media Utilization</h2>
@foreach (var item in utilization)
{
    <p>MediaID: @item.Key || Utilization: @item.Value</p>
}